<CarouselPage xmlns="http://xamarin.com/schemas/2014/forms"
              xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
               xmlns:i18n="clr-namespace:Service.Localization;assembly=GHQ"
               xmlns:controls="clr-namespace:Controls;assembly=ProjectX.Common"
               Title="{i18n:Translate PageHeader_ServiceRegistration}"
               xmlns:b="clr-namespace:Xamarin.Behaviors;assembly=Xamarin.Behaviors"
              x:Name="MainPage"
             x:Class="GHQ.UI.Pages.ServiceRegistration.ServiceRegistrationPage">


  <ContentPage Title="attachments">

    <RelativeLayout>
      <Image  Opacity="0.3"  Aspect="AspectFill"
                  RelativeLayout.WidthConstraint=
                  "{ConstraintExpression Type=RelativeToParent, Property=Width}"
                  RelativeLayout.HeightConstraint=
                  "{ConstraintExpression Type=RelativeToParent, Property=Height}">
        <Image.Source>
          <OnPlatform x:TypeArguments="ImageSource" WinPhone="Assets/background.png" Android="background.png" iOS="background"/>
        </Image.Source>
      </Image>

      <Grid Style="{StaticResource MainGridStyle}" RelativeLayout.WidthConstraint=
              "{ConstraintExpression Type=RelativeToParent, Property=Width}"
              RelativeLayout.HeightConstraint=
              "{ConstraintExpression Type=RelativeToParent, Property=Height}">

        <Grid.RowDefinitions>
          <RowDefinition Height="60" />
          <RowDefinition Height="Auto" />
          <RowDefinition Height="*" />
          <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>

        <Grid  Grid.Row="0" BackgroundColor="{StaticResource GoldColor}">
          <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
          </Grid.RowDefinitions>

          <Label  Grid.Row="0" TextColor="White" Text="{i18n:TranslateExtension Text=ServiceRegistration_Step7Header}" HorizontalOptions="Center"  VerticalOptions="Center"></Label>

          <Image  Grid.Row="1" HorizontalOptions="Center" VerticalOptions="Center"  Aspect="AspectFit" >
            <Image.Source>
              <OnPlatform x:TypeArguments="ImageSource" WinPhone="RegisterationStep7.png" Android="RegisterationStep7.png" iOS="RegisterationStep7"/>
            </Image.Source>
          </Image>
        </Grid>

        <ActivityIndicator Grid.Row="1" IsRunning="{Binding IsLoading}"  Style="{StaticResource ActivityIndicatorStyle}"/>


        <Grid  Grid.Row="2" Style="{StaticResource GridContentStyle}">

          <ListView IsVisible="false" x:Name="attachmentList" ItemsSource="{Binding ServiceAttachementData}" SelectedItem="{Binding AddedRegistrationattachment,Mode=TwoWay}" RowHeight="100" BackgroundColor="{StaticResource NewsTemplateBGColor}" >
            <ListView.ItemTemplate>
              <DataTemplate>
                <ViewCell>
                  <ViewCell.View>
                    <Grid Padding="20,10"  BackgroundColor="{StaticResource NewsTemplateBGColor}" >
                      <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                      </Grid.RowDefinitions>
                      <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="40" />
                        <ColumnDefinition Width="*" />

                      </Grid.ColumnDefinitions>

                      <Label Grid.Column="1" Grid.Row="0" TextColor="Black" Text="{Binding NameAr}" HorizontalTextAlignment="End"/>
                      <Label Grid.Column="1" Grid.Row="1" Margin="10,0,0,0" TextColor="Gray" Text="{Binding Attachment.FileName}" HorizontalTextAlignment="End"/>

                      <Image  Grid.RowSpan="2" Grid.Column="0" Style="{StaticResource EditImageStyle}" >
                        <Image.Source>
                          <OnPlatform x:TypeArguments="ImageSource" WinPhone="edit.png" Android="edit.png" iOS="edit"/>
                        </Image.Source>
                      </Image>

                    </Grid>
                  </ViewCell.View>
                </ViewCell>
              </DataTemplate>
            </ListView.ItemTemplate>
          </ListView>


          <Label Text="{i18n:Translate ServiceRegistration_NoAttachment}" Style="{StaticResource LabelHeaderEntryStyle}" HorizontalTextAlignment="Center" TextColor="Black"></Label>


        </Grid>


        <Grid  Grid.Row="3"  HorizontalOptions="FillAndExpand" >
          <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
          </Grid.ColumnDefinitions>

          <Button  Grid.Column="0" Style="{StaticResource ButtonSolidStyle}" IsEnabled="{Binding IsPageEnabled}" VerticalOptions="End" Text="{i18n:TranslateExtension Text=ServiceRegistration_Btn_Save}"  Command="{Binding OnStep7SaveCommand}"></Button>
          <Button  Grid.Column="1" Style="{StaticResource ButtonSolidStyle}" IsEnabled="{Binding IsPageEnabled}" VerticalOptions="End" Text="{i18n:TranslateExtension Text=ServiceRegistration_Btn_Submit}"  Command="{Binding OnFinishCommand}"></Button>

        </Grid>



      </Grid>

    </RelativeLayout>

  </ContentPage>

  <ContentPage Title="qualifications">
    <RelativeLayout>

      <Image  Opacity="0.3"  Aspect="AspectFill"
                  RelativeLayout.WidthConstraint=
                  "{ConstraintExpression Type=RelativeToParent, Property=Width}"
                  RelativeLayout.HeightConstraint=
                  "{ConstraintExpression Type=RelativeToParent, Property=Height}">
        <Image.Source>
          <OnPlatform x:TypeArguments="ImageSource" WinPhone="Assets/background.png" Android="background.png" iOS="background"/>
        </Image.Source>
      </Image>

      <Grid Style="{StaticResource MainGridStyle}" RelativeLayout.WidthConstraint=
              "{ConstraintExpression Type=RelativeToParent, Property=Width}"
              RelativeLayout.HeightConstraint=
              "{ConstraintExpression Type=RelativeToParent, Property=Height}">

        <Grid.RowDefinitions>
          <RowDefinition Height="60" />
          <RowDefinition Height="Auto" />
          <RowDefinition Height="*" />
          <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>

        <Grid  Grid.Row="0" BackgroundColor="{StaticResource GoldColor}">
          <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
          </Grid.RowDefinitions>

          <Label  Grid.Row="0" TextColor="White" Text="{i18n:TranslateExtension Text=ServiceRegistration_Step6Header}" HorizontalOptions="Center"  VerticalOptions="Center"></Label>

          <Image  Grid.Row="1" HorizontalOptions="Center" VerticalOptions="Center"  Aspect="AspectFit" >
            <Image.Source>
              <OnPlatform x:TypeArguments="ImageSource" WinPhone="RegisterationStep6.png" Android="RegisterationStep6.png" iOS="RegisterationStep6"/>
            </Image.Source>
          </Image>
        </Grid>

        <ActivityIndicator Grid.Row="1" IsRunning="{Binding IsLoading}"   Style="{StaticResource ActivityIndicatorStyle}"/>

        <Grid  Grid.Row="2" Style="{StaticResource GridContentStyle}">

          <ScrollView>

            <StackLayout Style="{StaticResource FormStackLayoutStyle}" Orientation="Vertical" >

              <StackLayout Orientation="Horizontal" HorizontalOptions="End" Margin="0,0,0,15">

                <Label TextColor="White" Text="{i18n:TranslateExtension Text=ServiceRegistration_Header_AcademicQualification}" HorizontalOptions="End"  VerticalOptions="Center"></Label>

                <Image Style="{StaticResource HeaderImageStyle}">
                  <Image.Source>
                    <OnPlatform x:TypeArguments="ImageSource" WinPhone="education.png" Android="education.png" iOS="education"/>
                  </Image.Source>
                </Image>

              </StackLayout>

              <Label   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.SelectAcademicQualificationname_IsVisible}}" Text="{i18n:Translate ServiceRegistration_Picker_AcademicQualification}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
              <controls:BindablePicker IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.SelectAcademicQualificationname_IsVisible}" ItemsSource="{Binding AcademicQualificationList}" Style="{StaticResource PickerStyle}" SelectedIndex="{Binding SelectedAcademicQualificationListIndex,Mode=TwoWay}"   SelectedItem="{Binding AddedAcademicqualification.SelectedAcademicQualification,Mode=TwoWay}"/>

              <StackLayout Orientation="Vertical" IsVisible="{Binding HasAcademicQualfication}}">

                <Label   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.SelectAcademicCountry_IsVisible}}" Text="{i18n:Translate ServiceRegistration_Picker_Country}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <controls:BindablePicker  IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.SelectAcademicCountry_IsVisible}" ItemsSource="{Binding AcademicCountryList}" Style="{StaticResource PickerStyle}" SelectedIndex="{Binding SelectedAcademicCountryListIndex,Mode=TwoWay}"   SelectedItem="{Binding AddedAcademicqualification.SelectedAcademicCountry,Mode=TwoWay}"/>

                <Label   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.SelectEducationInstitute_IsVisible}}" Text="{i18n:Translate ServiceRegistration_Picker_EducationJob}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <controls:BindablePicker  IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.SelectEducationInstitute_IsVisible}" ItemsSource="{Binding EducationInstitutesList}" Style="{StaticResource PickerStyle}" SelectedIndex="{Binding SelectedEducationInstitutesIndex,Mode=TwoWay}"   SelectedItem="{Binding AddedAcademicqualification.SelectedEducationInstitute,Mode=TwoWay}"/>

                <StackLayout IsVisible="{Binding IsHigh}}">

                  <Label   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.SelectMainSpecialization_IsVisible}}" Text="{i18n:Translate ServiceRegistration_Picker_MainSpecialization}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                  <controls:BindablePicker  IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.SelectMainSpecialization_IsVisible}" ItemsSource="{Binding MajorSpecialtyList}" Style="{StaticResource PickerStyle}" SelectedIndex="{Binding SelectedMajorSpecialtyListIndex,Mode=TwoWay}"   SelectedItem="{Binding AddedAcademicqualification.SelectedMainSpecialization,Mode=TwoWay}"/>

                </StackLayout>

                <StackLayout IsVisible="{Binding IsPreparatory,Converter={StaticResource BooleanNegateConverter}}">

                  <Label   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.SelectSubSpecialization_IsVisible}}" Text="{i18n:Translate ServiceRegistration_Picker_SubSpecialization}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                  <controls:BindablePicker  IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.SelectSubSpecialization_IsVisible}" ItemsSource="{Binding SecondarySpecialtyList}" Style="{StaticResource PickerStyle}" SelectedIndex="{Binding SelectedSecondarySpecialtyListIndex,Mode=TwoWay}"   SelectedItem="{Binding AddedAcademicqualification.SelectedMainSpecialization,Mode=TwoWay}"/>

                </StackLayout>

                <Label   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.GraduationDate_IsVisible}}" Text="{i18n:Translate ServiceRegistration_Picker_GraduationDate}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <DatePicker IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.GraduationDate_IsVisible}" Date="{Binding AddedAcademicqualification.GraduationDate}" Style="{StaticResource DatePickerStyle}"/>

                <StackLayout IsVisible="{Binding IsHigh}}">

                  <Label   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.GPA_IsVisible}}" Text="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_GPA}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                  <Entry Keyboard ="Numeric" IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.GPA_IsVisible}" Placeholder="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_GPA}" Text="{Binding AddedAcademicqualification.GPA}"  Style="{StaticResource EntryStyle}"/>

                </StackLayout>

                <StackLayout IsVisible="{Binding IsHigh,Converter={StaticResource BooleanNegateConverter}}">

                  <Label   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.GPA_IsVisible}}" Text="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_Percentage}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                  <Entry Keyboard ="Numeric" IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.GPA_IsVisible}" Placeholder="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_Percentage}" Text="{Binding AddedAcademicqualification.GPA}"  Style="{StaticResource EntryStyle}"/>

                </StackLayout>

                <StackLayout IsVisible="{Binding IsGradeVisible}}">

                  <Label   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.Grade_IsVisible}}" Text="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_Grade}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                  <Entry Keyboard ="Numeric" IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.Grade_IsVisible}" Placeholder="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_Grade}" Text="{Binding AddedAcademicqualification.Grade}"  Style="{StaticResource EntryStyle}"/>

                </StackLayout>

                <!--<Label   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.SelectJobLevel_IsVisible}}" Text="{i18n:Translate ServiceRegistration_Picker_JobLevel}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <controls:BindablePicker  IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.SelectJobLevel_IsVisible}" ItemsSource="{Binding WorkEntityList}" Style="{StaticResource PickerStyle}" SelectedIndex="{Binding SelectedWorkEntityListIndex,Mode=TwoWay}"   SelectedItem="{Binding AddedAcademicqualification.SelectedJobLevel,Mode=TwoWay}"/>-->

                <Grid IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.EducationFinished_IsVisible}">
                  <Label  Text="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_EducationFinished}" Style="{StaticResource RadioButtonLabelStyle}"/>
                  <Switch IsToggled="{Binding RegistrationData.ISEducationFinishedValue,Mode=TwoWay}" Style="{StaticResource RadioButtonStyle}"/>
                </Grid>

                <Label   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.Notes_IsVisible}}" Text="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_Notes}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <Entry IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.Notes_IsVisible}"  Placeholder="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_Notes}" Text="{Binding AddedAcademicqualification.Notes}"  Style="{StaticResource MultiLineEntryStyle}"/>

              </StackLayout>

            </StackLayout>

          </ScrollView>

        </Grid>

        <Button Grid.Row="3" Style="{StaticResource ButtonSolidStyle}" IsEnabled="{Binding IsPageEnabled}" VerticalOptions="End" Text="{i18n:TranslateExtension Text=ServiceRegistration_Btn_Save}"  Command="{Binding OnStep6SaveCommand}"></Button>

      </Grid>

    </RelativeLayout>
  </ContentPage>

  <ContentPage Title="wife">
    <RelativeLayout>
      <Image  Opacity="0.3"  Aspect="AspectFill"
                  RelativeLayout.WidthConstraint=
                  "{ConstraintExpression Type=RelativeToParent, Property=Width}"
                  RelativeLayout.HeightConstraint=
                  "{ConstraintExpression Type=RelativeToParent, Property=Height}">
        <Image.Source>
          <OnPlatform x:TypeArguments="ImageSource" WinPhone="Assets/background.png" Android="background.png" iOS="background"/>
        </Image.Source>
      </Image>

      <Grid Style="{StaticResource MainGridStyle}" RelativeLayout.WidthConstraint=
              "{ConstraintExpression Type=RelativeToParent, Property=Width}"
              RelativeLayout.HeightConstraint=
              "{ConstraintExpression Type=RelativeToParent, Property=Height}">

        <Grid.RowDefinitions>
          <RowDefinition Height="60" />
          <RowDefinition Height="Auto" />
          <RowDefinition Height="*" />
          <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>

        <Grid  Grid.Row="0" BackgroundColor="{StaticResource GoldColor}">
          <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
          </Grid.RowDefinitions>

          <Label  Grid.Row="0" TextColor="White" Text="{i18n:TranslateExtension Text=ServiceRegistration_Step5Header}" HorizontalOptions="Center"  VerticalOptions="Center"></Label>

          <Image  Grid.Row="1" HorizontalOptions="Center" VerticalOptions="Center"  Aspect="AspectFit" >
            <Image.Source>
              <OnPlatform x:TypeArguments="ImageSource" WinPhone="RegisterationStep5.png" Android="RegisterationStep5.png" iOS="RegisterationStep5"/>
            </Image.Source>
          </Image>
        </Grid>

        <ActivityIndicator Grid.Row="1" IsRunning="{Binding IsLoading}" Style="{StaticResource ActivityIndicatorStyle}"/>

        <Grid  Grid.Row="2" Style="{StaticResource GridContentStyle}" VerticalOptions="Center" HorizontalOptions="Center"  IsVisible="{Binding WifeTabVisible,Converter={StaticResource BooleanNegateConverter}}">

          <Label Text="{i18n:Translate ServiceRegistration_NoWife}" Style="{StaticResource LabelHeaderEntryStyle}" HorizontalTextAlignment="Center" TextColor="Black"></Label>

        </Grid>

        <Grid  Grid.Row="2" Style="{StaticResource GridContentStyle}" IsVisible="{Binding WifeTabVisible}">

          <ScrollView>
            <Grid >

              <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
              </Grid.RowDefinitions>

              <Label Grid.Row="0" Text="{i18n:Translate ServiceRegistration_ChooseWife}" Style="{StaticResource LabelHeaderEntryStyle}" TextColor="Black"> </Label>

              <controls:BindablePicker Grid.Row="1"  ItemsSource="{Binding RegistrationData.WifeHusbandRelativesWithoutDeleted}" SelectedIndex="{Binding SelectedWifeListIndex,Mode=TwoWay}" Style="{StaticResource PickerStyle}"   SelectedItem="{Binding AddedWife,Mode=TwoWay}"/>

              <StackLayout Grid.Row="2" Style="{StaticResource FormStackLayoutStyle}" Orientation="Vertical" >

                <Label  Text="{i18n:Translate ServiceRegistration_Picker_RelativeType}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <controls:BindablePicker ItemsSource="{Binding WifeTypesList}" Style="{StaticResource PickerStyle}" SelectedIndex="{Binding SelectedWifeTypesListIndex,Mode=TwoWay}"   SelectedItem="{Binding AddedWife.SelectedRelativetype,Mode=TwoWay}"/>


                <StackLayout Orientation="Horizontal" HorizontalOptions="End" Margin="0,0,0,15">

                  <Label TextColor="White" Text="{i18n:TranslateExtension Text=ServiceRegistration_Header_NameInArabic}" HorizontalOptions="End"  VerticalOptions="Center"></Label>

                  <Image Style="{StaticResource HeaderImageStyle}">
                    <Image.Source>
                      <OnPlatform x:TypeArguments="ImageSource" WinPhone="personal.png" Android="personal.png" iOS="personal"/>
                    </Image.Source>
                  </Image>

                </StackLayout>

                <Label   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.A_FirstName_Relative_IsVisible}}" Text="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_ArabicFirstName}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <Entry  IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.A_FirstName_Relative_IsVisible}" Placeholder="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_ArabicFirstName}"  Text="{Binding AddedWife.FirstName_Arabic}"  Style="{StaticResource EntryStyle}"/>

                <Label   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.A_SecondName_Relative_IsVisible}}" Text="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_ArabicSecondName}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <Entry  IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.A_SecondName_Relative_IsVisible}"  Placeholder="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_ArabicSecondName}"  Text="{Binding AddedWife.SecondName_Arabic}"  Style="{StaticResource EntryStyle}"/>

                <Label   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.A_ThirdName_Relative_IsVisible}}" Text="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_ArabicThirdName}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <Entry   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.A_ThirdName_Relative_IsVisible}" Placeholder="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_ArabicThirdName}"  Text="{Binding AddedWife.ThirdName_Arabic}"  Style="{StaticResource EntryStyle}"/>

                <Label   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.A_FourthName_Relative_IsVisible}}" Text="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_ArabicFourthName}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <Entry   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.A_FourthName_Relative_IsVisible}" Placeholder="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_ArabicFourthName}"  Text="{Binding AddedWife.FourthName_Arabic}"  Style="{StaticResource EntryStyle}"/>

                <Label   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.A_FifthName_Relative_IsVisible}}" Text="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_ArabicFifthName}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <Entry  IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.A_FifthName_Relative_IsVisible}" Placeholder="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_ArabicFifthName}"  Text="{Binding AddedWife.FifthName_Arabic}"  Style="{StaticResource EntryStyle}"/>


                <StackLayout Orientation="Horizontal" HorizontalOptions="End" Margin="0,0,0,15">

                  <Label TextColor="White" Text="{i18n:TranslateExtension Text=ServiceRegistration_Header_NameInEnglish}" HorizontalOptions="End"  VerticalOptions="Center"></Label>

                  <Image Style="{StaticResource HeaderImageStyle}">
                    <Image.Source>
                      <OnPlatform x:TypeArguments="ImageSource" WinPhone="personal.png" Android="personal.png" iOS="personal"/>
                    </Image.Source>
                  </Image>

                </StackLayout>

                <Label   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.E_FirstName_Relative_IsVisible}}" Text="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_EnglishFirstName}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <Entry  IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.E_FirstName_Relative_IsVisible}" Placeholder="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_EnglishFirstName}" Text="{Binding AddedWife.FirstName_English}"  Style="{StaticResource EntryStyle}"/>

                <Label   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.E_SecondName_Relative_IsVisible}}" Text="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_EnglishSecondName}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <Entry   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.E_SecondName_Relative_IsVisible}" Placeholder="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_EnglishSecondName}" Text="{Binding AddedWife.SecondName_English}"  Style="{StaticResource EntryStyle}"/>

                <Label   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.E_ThirdName_Relative_IsVisible}}" Text="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_EnglishThirdName}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <Entry   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.E_ThirdName_Relative_IsVisible}" Placeholder="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_EnglishThirdName}" Text="{Binding AddedWife.ThirdName_English}"  Style="{StaticResource EntryStyle}"/>

                <Label   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.E_FourthName_Relative_IsVisible}}" Text="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_EnglishFourthName}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <Entry   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.E_FourthName_Relative_IsVisible}" Placeholder="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_EnglishFourthName}" Text="{Binding AddedWife.FourthName_English}"  Style="{StaticResource EntryStyle}"/>

                <Label   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.E_FifthName_Relative_IsVisible}}" Text="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_EnglishFifthName}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <Entry   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.E_FifthName_Relative_IsVisible}" Placeholder="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_EnglishFifthName}" Text="{Binding AddedWife.FifthName_English}"  Style="{StaticResource EntryStyle}"/>

                <StackLayout Orientation="Horizontal" HorizontalOptions="End" Margin="0,0,0,15">

                  <Label TextColor="White" Text="{i18n:TranslateExtension Text=ServiceRegistration_Header_GeneralInfo}" HorizontalOptions="End"  VerticalOptions="Center"></Label>

                  <Image HorizontalOptions="End" VerticalOptions="Center"  Aspect="AspectFit" WidthRequest="40" HeightRequest="40">
                    <Image.Source>
                      <OnPlatform x:TypeArguments="ImageSource" WinPhone="personal.png" Android="personal.png" iOS="personal"/>
                    </Image.Source>
                  </Image>

                </StackLayout>

                <Label   Text="{i18n:Translate ServiceRegistration_Picker_NationalityCurrent}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <controls:BindablePicker ItemsSource="{Binding WifeNationalityList}" SelectedIndex="{Binding SelectedWifeNationalityListIndex,Mode=TwoWay}" SelectedItem="{Binding AddedWife.SelectedNationality,Mode=TwoWay}" Style="{StaticResource PickerStyle}"/>

                <Label  Text="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_WorkPlace}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <Entry   Placeholder="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_WorkPlace}" Text="{Binding AddedWife.WorkPlace}"  Style="{StaticResource EntryStyle}"/>

                <Label  Text="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_JobName}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <Entry   Placeholder="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_JobName}" Text="{Binding AddedWife.JobName}"  Style="{StaticResource EntryStyle}"/>

                <Label   Text="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_BirthPlace}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <Entry   Placeholder="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_BirthPlace}" Text="{Binding AddedWife.BirthPlace}"  Style="{StaticResource EntryStyle}"/>

                <Label   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.AgeofRelative_IsVisible}}" Text="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_Age}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <Entry  Keyboard ="Numeric" IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.AgeofRelative_IsVisible}" Placeholder="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_Age}" Text="{Binding AddedWife.AgeOfRelative}"  Style="{StaticResource EntryStyle}"/>

                <Label   Text="{i18n:Translate ServiceRegistration_Picker_LivingPlace}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <controls:BindablePicker ItemsSource="{Binding WifeEmairtesList}" SelectedIndex="{Binding SelectedWifeEmairtesListIndex,Mode=TwoWay}" SelectedItem="{Binding AddedWife.SelectedResidencePlaceID,Mode=TwoWay}" Style="{StaticResource PickerStyle}"/>

                <Label   Text="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_TribeName}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <controls:BindablePicker ItemsSource="{Binding WifeTribeList}" Style="{StaticResource PickerStyle}" SelectedIndex="{Binding SelectedWifeTribeListIndex,Mode=TwoWay}"   SelectedItem="{Binding AddedWife.SelectedTribe,Mode=TwoWay}"/>

                <Label   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.StatusofRelative_IsVisible}}" Text="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_RelativeStatus}" Style="{StaticResource LabelHeaderEntryStyle}"></Label>
                <controls:BindablePicker   IsVisible="{Binding RegistrationData.VisibilitiesAndRequiredFeilds.StatusofRelative_IsVisible}" ItemsSource="{Binding WifeStatusList}" SelectedIndex="{Binding SelectedWifeStatusListIndex,Mode=TwoWay}" SelectedItem="{Binding AddedWife.SelectedStatusOfRelative,Mode=TwoWay}" Style="{StaticResource PickerStyle}"/>

                <Grid>
                  <Label  Text="{i18n:Translate ServiceRegistration_Entry_PlaceHolder_IsAlive}" Style="{StaticResource RadioButtonLabelStyle}"/>
                  <Switch IsToggled="{Binding AddedWife.IsAliveValue}" Style="{StaticResource RadioButtonStyle}"/>
                </Grid>

              </StackLayout>

              <StackLayout Grid.Row="3"  Orientation="Horizontal" HorizontalOptions="Center" IsVisible="{Binding AddedWifeAddMode,Converter={StaticResource BooleanNegateConverter}}}">

                <Button Style="{StaticResource ButtonSolidStyle_Small}"  HorizontalOptions="Center"  VerticalOptions="End" Text="{i18n:TranslateExtension Text=ServiceRegistration_Btn_EditRelative}"  Command="{Binding OnAddNewWifeCommand}"></Button>

                <Button Style="{StaticResource ButtonSolidStyle_Small}"  HorizontalOptions="Center" VerticalOptions="End" Text="{i18n:TranslateExtension Text=ServiceRegistration_Btn_DeleteRelative}"  Command="{Binding OnDeleteWifeCommand}"></Button>

              </StackLayout>

              <StackLayout Grid.Row="3"  Orientation="Horizontal" HorizontalOptions="Center" IsVisible="{Binding AddedWifeAddMode}}" >

                <Button Style="{StaticResource ButtonSolidStyle_Small}"  HorizontalOptions="Center"  VerticalOptions="End" Text="{i18n:TranslateExtension Text=ServiceRegistration_Btn_AddWife}"  Command="{Binding OnAddNewWifeCommand}"></Button>

              </StackLayout>

            </Grid>
          </ScrollView>

        </Grid>

        <Button Grid.Row="3" Style="{StaticResource ButtonSolidStyle}" IsVisible="{Binding WifeTabVisible}" IsEnabled="{Binding IsPageEnabled}" VerticalOptions="End" Text="{i18n:TranslateExtension Text=ServiceRegistration_Btn_Save}"  Command="{Binding OnStep5SaveCommand}"></Button>

      </Grid>

    </RelativeLayout>
  </ContentPage>

  <ContentPage Title="relatives">
    <RelativeLayout>
      <Image  Opacity="0.3"  Aspect="AspectFill"
                  RelativeLayout.WidthConstraint=
                  "{ConstraintExpression Type=RelativeToParent, Property=Width}"
                  RelativeLayout.HeightConstraint=
                  "{ConstraintExpression Type=RelativeToParent, Property=Height}">
        <Image.Source>
          <OnPlatform x:TypeArguments="ImageSource" WinPhone="Assets/background.png" Android="background.png" iOS="background"/>
        </Image.Source>
      </Image>

      <Grid Style="{StaticResource MainGridStyle}" RelativeLayout.WidthConstraint=
